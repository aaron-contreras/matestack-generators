class <%= @ruby_component_config[:klass] %> < <%= @ruby_component_config[:base_klass] %>
  vue_name "<%= @vue_js_component_config[:registry_name] %>"

  <%- unless options[:full] -%>
  def response
    div do
      plain "This is <%= @ruby_component_config[:klass] %>"
    end
  end

  def vue_props
    {}.tap do |config|
    end
  end
  <%- else -%>
  # required :some_required_property
  # optional :some_optional_property

  def prepare
  end

  def render?
    true
  end

  def response
    div do
      my_partial
    end
  end

  def vue_props
    {}.tap do |config|
    end
  end

  private

  def my_partial
    paragraph "This is <%= @ruby_component_config[:klass] %>"
  end
  <%- end -%>
end
